<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.raktseva">

    <uses-permission android:name="android.permission.CALL_PHONE"/>
    <application

        android:allowBackup="true"
        android:icon="@drawable/rakt_seva_logo_squared"
        android:label="@string/app_name"
        android:roundIcon="@drawable/rakt_seva_logo_rounded"
        android:supportsRtl="true"
        android:theme="@style/Theme.RaktSeva">
        <activity android:name=".UserProfileActivity"
            android:parentActivityName=".MainMenuActivity"/>
        <activity android:name=".AboutActivity" />
        <activity android:name=".GetUserDetailsActivity" />
        <activity android:name=".MainMenuActivity" />
        <activity android:name=".AdminLoginActivity" />
        <activity android:name=".LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>